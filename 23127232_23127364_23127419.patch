+char * get_filename(char* path)
+{
+    char* p;
+    for(p= path + strlen(path); p >= path && *p != '/'; p-- );
+    return p + 1;
+}
+void search_file_recursive(char *dir_path, char *target_name)
+{
+    char buf[512], *p;
+    int fd;
+    struct dirent de;
+    struct stat st;
+
+    if (!strcmp(get_filename(dir_path), target_name))
+    {
+        printf("%s\n", dir_path);
+    }
+    if((fd = open(dir_path, O_RDONLY)) < 0)
+    {
