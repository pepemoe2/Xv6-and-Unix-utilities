+        // Tiáº¿n trÃ¬nh con
+        close(new_pipe[1]);
+        close(read_fd);
+        sieve(new_pipe[0]);
+        close(new_pipe[0]);
+        exit(0);
+    } 
+    else
+    {
+        // Tiáº¿n trÃ¬nh cha
+        close(new_pipe[0]);
+
+        while (read(read_fd, &current, sizeof(int))) {
