+void sieve(int) __attribute__((noreturn));
+
+void sieve(int read_fd) 
+{
+    int first, current;
+    int new_pipe[2];
+
+    if (read(read_fd, &first, sizeof(int)) == 0) 
+    {
+        close(read_fd);
+        exit(0);
+    }
+
+    printf("prime %d\n", first);
+
+    if (pipe(new_pipe) < 0) {
+        printf("Error: Pipe creation failed\n");
+        close(read_fd);
+        exit(1);
+    }
+
+    int child_pid = fork();
+    if (child_pid < 0) 
+    {
+        printf("Error: Fork failed\n");
+        close(read_fd);
+        close(new_pipe[0]);
+        close(new_pipe[1]);
+        exit(1);
+    } 
+    else if (child_pid == 0) 
